{
  "name": "postjss-counter-example",
  "version": "0.0.1",
  "description": "Counter example for PostJSS",
  "main": "lib/server/index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./tools/webpack.config.dev.js --progress",
    "start:server": "BABEL_ENV=server BABEL_DISABLE_CACHE=1 babel-node src/server",
    "clean:server": "rm -rf lib",
    "clean:client": "rm -rf dist",
    "build:client": "npm run clean:client && webpack --config ./tools/webpack.config.client.js",
    "build:server": "npm run clean:server && BABEL_ENV=server babel src --copy-files --out-dir ./lib",
    "build": "npm run build:client && npm run build:server"
  },
  "author": "Kenzhaev Artur",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "express": "^4.14.1",
    "postjss": "^0.0.5",
    "pug": "^2.0.0-beta9",
    "ramda": "^0.23.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-jss": "^5.3.0",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.0"
  },
  "devDependencies": {
    "@lttb/configs.style": "lttb/configs#style",
    "@lttb/eslint-config-default": "https://github.com/lttb/configs#js",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-async-to-promises": "^1.0.5",
    "babel-plugin-css-modules-transform": "^1.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-root-import": "^4.1.5",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-loader": "^1.6.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "html-webpack-plugin": "^2.26.0",
    "postcss-js": "^0.2.0",
    "postcss-loader": "^1.3.0",
    "postcss-reporter": "^3.0.0",
    "postcss-scss": "^0.4.1",
    "precss": "^1.4.0",
    "pug-loader": "^2.3.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "style-loader": "^0.13.1",
    "stylelint": "^7.8.0",
    "sugarss": "^0.2.0",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^2.3.0",
    "webpack-hot-middleware": "^2.15.0"
  }
}
